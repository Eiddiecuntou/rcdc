<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <groupId>com.ruijie.rcos.sk.root</groupId>
        <artifactId>sk-root-api</artifactId>
       	<version>0.0.0-SNAPSHOT</version>
    </parent>
    <version>0.0.0-SNAPSHOT</version>
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.ruijie.rcos.rcdc.terminal.module</groupId>
    <artifactId>rcdc-terminal-module</artifactId>
    <packaging>pom</packaging>
    <name>rcdc-terminal-module</name>
    <modules>
        <module>rcdc-terminal-module-def</module>
        <module>rcdc-terminal-module-impl</module>
        <module>rcdc-terminal-module-web</module>
    </modules>
    
    <properties>
    	<base-aaa-module.version>0.0.0-SNAPSHOT</base-aaa-module.version>
    	<base-sysmanage-module.version>0.0.0-SNAPSHOT</base-sysmanage-module.version>
    </properties>

    <!--定义依赖包版本号-->
    <dependencies>
        <dependency>
            <groupId>org.jmockit</groupId>
            <artifactId>jmockit</artifactId>
            <scope>test</scope>
        </dependency>

        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.1</version>
                <configuration>
                    <verbose>true</verbose>
                    <fork>true</fork>
                    <executable>${JAVA8_HOME}/bin/javac</executable>
                </configuration>
            </plugin>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.4.2</version>
                <configuration>
                    <skipTests>true</skipTests>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
